            <TabItem Header="6-18岁男">
                <Image Margin="4" Source="../../Data/BMI/boy5-19.png" />
            </TabItem>
            <TabItem Header="6-18岁女">
                <Image Margin="4" Source="../../Data/BMI/girl5-19.png" />
